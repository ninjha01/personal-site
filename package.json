{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf ./build && NODE_ENV=production NEXT_PUBLIC_GIT_COMMIT_SHA=`git rev-parse --short HEAD` next build",
    "start": "next start -p 8080",
    "serve": "pnpm build && pnpm start",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint": "tsc && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@vercel/speed-insights": "^1.0.0",
    "cors": "^2.8.5",
    "isarray": "^2.0.5",
    "next": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "reactflow": "^11.9.3",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.30.7",
    "wretch": "^2.6.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/cypress": "^8.0.3",
    "@types/cors": "^2.8.13",
    "@types/node": "17.0.42",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-next": "^13.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^3.1.2",
    "typescript": "4.7.3"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
