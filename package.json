{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "@headlessui/react": "^1.6.3",
  "@heroicons/react": "^1.0.6",
  "cypress": "^10.0.2",
  "eslint-plugin-cypress": "^2.12.1",
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf ./build && NODE_ENV=production NEXT_PUBLIC_GIT_COMMIT_SHA=`git rev-parse --short HEAD` next build",
    "start": "next start -p 8080",
    "serve": "yarn build && yarn start",
    "lint": "next lint",
    "format": "prettier \"**/*.+(js|jsx|ts|tsx|json|css|md)\" --config package.json --write",
    "format:check": "prettier \"**/*.+(js|jsx|ts|tsx|json|css|md)\" --config package.json --check",
    "cypress": "cypress open --config baseUrl=\"localhost:3000\"",
    "cypress:check": "cypress run --config baseUrl=\"localhost:3000\"",
    "cypress:staging": "cypress open --config baseUrl=\"https://staging.nishantjha.org\""
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "cors": "^2.8.5",
    "isarray": "^2.0.5",
    "next": "^13.1.1",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.6",
    "react-markdown": "^8.0.3",
    "remark-gfm": "^3.0.1",
    "seqviz": "^3.3.2",
    "sharp": "^0.30.7",
    "wretch": "^2.6.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/cypress": "^8.0.3",
    "@types/cors": "^2.8.13",
    "@types/node": "17.0.42",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "cypress": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-config-next": "^13.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^3.1.2",
    "typescript": "4.7.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120
  }
}
