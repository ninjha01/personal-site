{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf ./build && NODE_ENV=production NEXT_PUBLIC_GIT_COMMIT_SHA=`git rev-parse --short HEAD` next build",
    "start": "next start -p 8080",
    "serve": "pnpm build && pnpm start",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint": "tsc && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@vercel/speed-insights": "^1.0.2",
    "cors": "^2.8.5",
    "isarray": "^2.0.5",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "reactflow": "^11.10.1",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "wretch": "^2.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/cypress": "^10.0.1",
    "@types/cors": "^2.8.17",
    "@types/node": "20.10.6",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.5.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "5.3.3"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
